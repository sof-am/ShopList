<div class="page-header">
  <h1>
    Kitchen
    <a href="/kitchen/new" class="btn btn-lg btn-success">New Place</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Image</th>
        <th>Place</th>
        <th>Actions</th>
      </tr>

      <% @lists.each do |list| %>
      <% if list.user_id = current_user.id %>
      <tr>
        <td><img src="<%= list.image %>" width="40"></td>
        <td><%= list.list_name %></td>
        <td>

          <a href="/kitchen/<%= list.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_kitchen/<%= list.id %>" class="btn btn-primary" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
      <% end %>
    </table>
  </div>
</div>
